         point="org.eclipse.ui.newWizards">
      <category
            id="net.karpisek.gemdev.ui.wizardsCategory"
            name="%wizardsCategory.name">
      </category>
      <wizard
            category="net.karpisek.gemdev.ui.wizardsCategory"
            class="net.karpisek.gemdev.ui.wizards.NewProjectWizard"
            finalPerspective="net.karpisek.gemdev.ui.browserPerspective"
            icon="resources/icons/project.gif"
            id="net.karpisek.gemdev.ui.newProjectWizard"
            name="%newProjectWizard.name"
            project="true">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            icon="resources/icons/project.gif"
            id="net.karpisek.gemdev.ui.projectNatureImage"
            natureId="net.karpisek.gemdev.core.projectNature">
      </image>
   </extension>
