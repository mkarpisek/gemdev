         point="org.eclipse.ui.propertyPages">
      <page
            class="net.karpisek.gemdev.ui.preferences.ProjectServerPropertyPage"
            id="net.karpisek.gemdev.ui.projectPropertyPage"
            name="%serverPropertyPage.name">
         <enabledWhen>
            <and>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
               <test
                     property="org.eclipse.core.resources.projectNature"
                     value="net.karpisek.gemdev.core.projectNature">
               </test>
            </and>
         </enabledWhen>
      </page>
      <page
            class="net.karpisek.gemdev.ui.preferences.ClassPropertyPage"
            id="net.karpisek.gemdev.ui.classPropertyPage"
            name="%classPropertyPage.name">
         <enabledWhen>
            <and>
               <instanceof
                     value="net.karpisek.gemdev.core.db.DbClass">
               </instanceof>
            </and>
         </enabledWhen>
      </page>
      <page
            class="net.karpisek.gemdev.ui.preferences.MethodPropertyPage"
            id="net.karpisek.gemdev.ui.methodPropertyPage"
            name="%methodPropertyPage.name">
         <enabledWhen>
            <and>
               <instanceof
                     value="net.karpisek.gemdev.core.db.DbMethod">
               </instanceof>
            </and>
         </enabledWhen>
      </page>
   </extension>
